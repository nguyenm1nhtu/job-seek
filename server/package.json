{
    "name": "server",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node ./bin/www",
        "dev": "nodemon ./bin/www",
        "test": "jest --detectOpenHandles",
        "migrate": "npx sequelize-cli db:migrate",
        "migrate:undo": "npx sequelize-cli db:migrate:undo",
        "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate",
        "seed": "npx sequelize-cli db:seed:all",
        "seed:undo": "npx sequelize-cli db:seed:undo",
        "seed:reset": "npx sequelize-cli db:seed:undo:all"
    },
    "dependencies": {
        "@google-cloud/storage": "^7.17.2",
        "bcryptjs": "^3.0.2",
        "compression": "^1.8.1",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "dotenv": "^17.2.3",
        "express": "^4.21.2",
        "helmet": "^8.1.0",
        "jsonb": "^0.1.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.1",
        "multer": "^2.0.2",
        "pg": "^8.16.3",
        "redis": "^5.8.3",
        "sequelize": "^6.37.7",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "jest": "^30.2.0",
        "nodemon": "^3.1.10",
        "sequelize-cli": "^6.6.3",
        "supertest": "^7.1.4"
    },
    "jest": {
        "testEnvironment": "node",
        "testMatch": [
            "**/tests/**/*.test.js",
            "**/?(*.)+(spec|test).js"
        ],
        "moduleFileExtensions": [
            "js",
            "json"
        ],
        "collectCoverageFrom": [
            "**/*.{js,json}",
            "!**/node_modules/**"
        ],
        "coverageDirectory": "coverage"
    }
}
